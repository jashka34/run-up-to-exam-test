    "kefir": "3.8.1",
    "morgan": "1.9.0",
    "popper.js": "^1.12.9",
    "pug": "^2.0.0-rc.4"
  },
  "devDependencies": {
    "@types/bootstrap": "^3.3.37",
    "@types/chai": "4.1.2",
    "@types/chai-http": "^3.0.3",
    "@types/kefir": "3.7.0",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "clean-webpack-plugin": "^0.1.18",
