  },
  "devDependencies": {
    "@types/chai": "4.1.2",
    "@types/chai-http": "^3.0.3",
    "@types/kefir": "3.7.0",
    "@types/mocha": "2.2.48",
    "@types/node": "9.4.1",
    "chai": "^4.1.2",
    "chai-http": "^3.0.0",
    "mocha": "^5.0.0",
    "pug-cli": "^1.0.0-alpha6",
    "ts-node": "^4.1.0",
    "typescript": "^2.7.1"
