{
  "name": "run-up-to-exam-test",
  "version": "0.0.1",
  "description": "Engine for preparation to exam test.",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git@github.com:jashka34/run-up-to-exam-test.git"
  },
  "scripts": {
    "build": "webpack",
    "watch:webpack": "webpack --watch",
    "watch:pug": "node_modules/pug-cli/index.js -w --out src src/views/*.pug",
    "start": "export NODE_ENV=dev && tsc && node dist/index.js",
    "compile": "tsc",
    "test": "mocha",
    "nyan-test": "mocha -R nyan",
    "watch:test": "export NODE_ENV=test && mocha -r ts-node/register --watch test/**/*.test.ts --watch-extensions ts"
  },
